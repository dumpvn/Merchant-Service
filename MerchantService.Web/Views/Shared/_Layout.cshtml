@*Add Ledger Popup*@
<script type="text/ng-template" id="ledgerAddPopup">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" name="close-btn" data-dismiss="modal" ng-click="cancleAddLedgerPopup()" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title"><span string-replace="addledger"></span></h4>
        </div>
        <div class="modal-body">
            <form name="addLedger">
                <div class="form-horizontal">
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="form-group row">
                                <label class="control-label col-lg-4"><span string-replace="category"></span>:</label>
                                <div class="col-lg-8" ng-class="{ 'has-error': addLedger.category.$touched && addLedger.category.$invalid }">
                                    <ui-select name="category" ng-model="ledgersAccount.GroupId" ng-change="changeCategory()" required>
                                        <ui-select-match placeholder="Select Category">{{$select.selected.GroupName}}</ui-select-match>
                                        <ui-select-choices repeat="group.GroupId as group in listOfGroup | filter: $select.search">
                                            <div ng-bind-html="group.GroupName | highlight: $select.search" name="{{group.GroupName}}"></div>
                                        </ui-select-choices>
                                    </ui-select>
                                    <span class="error">*</span>
                                    <div class="help-block" ng-messages="addLedger.category.$error" ng-if="addLedger.category.$touched">
                                        <p ng-message="required" class="text-error">{{groupNameRequired}}</p>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="control-label col-lg-4"><span string-replace="detailtype"></span>:</label>
                                <div class="col-lg-8" ng-class="{ 'has-error': addLedger.detailtype.$touched && addLedger.detailtype.$invalid }">
                                    <ui-select name="detailtype" ng-model="ledgersAccount.GroupTypeId" ng-disabled="isDisableDetailType" ng-change="changeDetailType(groupType.Name)" required>
                                        <ui-select-match placeholder="Select Detail">{{$select.selected.Name}}</ui-select-match>
                                        <ui-select-choices repeat="groupType.Id  as groupType in listOfLedgerGroupType | filter: $select.search">
                                            <div ng-bind-html="groupType.Name | highlight: $select.search" name="{{groupType.Name}}"></div>
                                        </ui-select-choices>
                                    </ui-select>
                                    <span class="error">*</span>
                                    <div class="help-block" ng-messages="addLedger.detailtype.$error" ng-if="addLedger.detailtype.$touched">
                                        <p ng-message="required" class="text-error">{{detailTypeRequired}}</p>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-lg-4"></label>
                                <div class="col-lg-8">
                                    <div class="display-text-according disabled">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group row">
                                <label class="control-label col-lg-4"><span string-replace="name"></span>:</label>
                                <div class="col-lg-8" ng-class="{ 'has-error': addLedger.ledegerName.$touched && addLedger.ledegerName.$invalid }">
                                    <input type="text" name="ledegerName" class="form-control" maxlength="255" ng-model="ledgersAccount.LedgerName" required />
                                    <span class="error">*</span>
                                    <div class="help-block" ng-messages="addLedger.ledegerName.$error" ng-if="addLedger.ledegerName.$touched">
                                        <p ng-message="required" class="text-error">{{ledegerNameRequired}}</p>
                                    </div>
                                    <p class="text-error" ng-show="isLedgerNameNotValid">{{ledgerNameNotValid}}</p>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="control-label col-lg-4"><span string-replace="description"></span>:</label>
                                <div class="col-lg-8">
                                    <textarea class="form-control" maxlength="250" ng-model="ledgersAccount.Description"></textarea>
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="checkbox col-lg-4 control-label"> <label> <input type="checkbox" ng-model="ledgersAccount.isSubAccountChecked" ng-click="clickSubAccountCheckBox()"><span string-replace="Is sub-account"></span></label> </div>
                                <div class="col-lg-8">
                                    <ui-select name="detailtype" ng-model="ledgersAccount.ParentLedgerId" ng-disabled="ledgersAccount.isSubAccountChecked?false:true">
                                        <ui-select-match placeholder="Select">{{$select.selected.LedgerName}}</ui-select-match>
                                        <ui-select-choices repeat="ledger.LedgerId as ledger in listOfLedgers  | filter: $select.search">
                                            <div>
                                                <label class="main-opt" ng-bind-html="ledger.LedgerName | highlight: $select.search" name="{{ledger.LedgerName}}"></label><label class="sub-option-name no-bold">{{ledger.LableName}}</label>
                                            </div>
                                        </ui-select-choices>
                                    </ui-select>
                                    <p class="text-error" ng-show="isLedgerValid">{{ledgerNotValid}}</p>
                                </div>
                            </div>
                            <div class="form-group row" ng-show="isBalancedSection">
                                <div class="col-lg-6 p-0">
                                    <label class="control-label"><span string-replace="balance"></span>:</label><br />
                                    <input type="text" class="form-control" maxlength="13" ng-pattern="/^0*(?!0\d|$)\d{1,8}(\.\d{1,4})?$/" ng-model="ledgersAccount.Balance" />
                                </div>
                                <div class="col-lg-6 p-0">
                                    <label class="control-label"><span string-replace="asOf"></span>:</label><br />
                                    <div class="input-group">
                                        <input type="text" name="end-date" disabled class="form-control" datepicker-popup="dd-MMMM-yyyy" ng-model="ledgersAccount.ledgersDate" is-open="isLedgerDate" close-text="Close" />
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-default" ng-click="openledgerDate($event)"><i class="glyphicon glyphicon-calendar"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" name="save-btn" class="btn btn-primary" ng-click="saveLedgers()" ng-disabled="addLedger.$invalid"><span string-replace="save"></span></button>
                    <button type="button" name="cancle-btn" class="btn btn-default" ng-click="cancleAddLedgerPopup()"><span string-replace="cancelpos"></span></button>
                </div>
            </form>
        </div>
    </div>
</script>

