                            <input type="text" class="form-control" id="" maxlength="255" name="flavour" ng-model="search.FlavourEn">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-4 p-0"><span string-replace="itemcode"></span>:</label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="" maxlength="20" name="code" ng-model="search.Code">
                        </div>
                    </div>
                </div>
            </div>
            <br />
            <div class="row text-center">
                <button class="btn btn-primary" name="search-btn" ng-click="serchItemDetails()"><span string-replace="search"></span></button>
            </div>
        </div>
        <br />
        <div class="alert alert-success text-center" role="alert" ng-show="errorItemMessage">
            <span>{{noItemFound}}</span>
        </div>
        <br />
        <div ng-hide="errorItemMessage">
            <br />
            <table class="table table-striped table-bordered table-hover" name="inventory-grid">
                <thead>
                    <tr>
                        <th></th>
                        <th><span string-replace="barcode"></span></th>
                        <th><span string-replace="itemname"></span></span> </th>
                        <th><span string-replace="itemcode"></span></th>
                        <th><span string-replace="flavourpos"></span></th>
                        <th><span string-replace="unit"></span></th>
                        <th><span string-replace="pricepos"></span></th>
                    </tr>
                </thead>
                <tbody dir-paginate="item in itemCollection| itemsPerPage: 8">
                    <tr>
                        <td id="{{item.Id}}"><a class="pointer" ng-click="getSubItemList(item.ItemProfileId)" name="plus-link" ng-show="item.HasChildItem"><i attr="item.Id" class="fa fa-plus action-icon"></i></a></td>
                        <td class="pointer" ng-click="getItemDetails(item.Barcode)">{{item.Barcode}}</td>
                        <td class="pointer" ng-click="getItemDetails(item.Barcode)">{{item.ItemNameEn}}</td>
                        <td class="pointer" ng-click="getItemDetails(item.Barcode)">{{item.Code}}</td>
                        <td class="pointer" ng-click="getItemDetails(item.Barcode)">{{item.FlavourEn}}</td>
                        <td class="pointer" ng-click="getItemDetails(item.Barcode)">{{item.Unit}}</td>
                        <td class="pointer" ng-click="getItemDetails(item.Barcode)">{{item.SellPrice}}</td>
                    </tr>
                    <tr id="subChild{{item.Id}}" ng-if="item.HasChildItem" class="isHide sub-grid-list">
                        <td colspan="10">
                            <table class="table table-striped table-bordered table-hover sub-table" name="sub-table">
                                <thead>
                                    <tr>
                                        <th><span string-replace="barcode"></span></th>
                                        <th><span string-replace="itemname"></span></span> </th>
                                        <th><span string-repalce="itemcode"></span></th>
                                        <th><span string-replace="flavourpos"></span></th>
                                        <th><span string-replace="unit"></span></th>
                                        <th><span string-replace="pricepos"></span></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr class="pointer" ng-repeat="subItem in item.listOfChildProfileAC" ng-click="getItemDetails(subItem.Barcode)">
                                        <td>{{subItem.Barcode}}</td>
                                        <td>{{subItem.ItemNameEn}}</td>
                                        <td>{{subItem.Code}}</td>
                                        <td>{{subItem.FlavourEn}}</td>
                                        <td>{{subItem.Unit}}</td>
                                        <td>{{subItem.SellPrice}}</td>
                                    </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>
                </tbody>
            </table>
            <div ng-show="itemCollection.length >0" style="text-align:center">
                <dir-pagination-controls boundary-links="true"></dir-pagination-controls>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button class="btn btn-default" type="button" name="cancel-btn" ng-click="closeItemDetailModelDialogBox()"><span string-replace="cancelpos"></span></button>
    </div>
</script>

