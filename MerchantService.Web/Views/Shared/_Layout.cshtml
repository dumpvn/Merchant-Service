                                        <div class="col-sm-8" ng-class="{'has-error': updateCustomer.balanceAmount.$touched && updateCustomer.balanceAmount.$invalid}">
                                            <input ng-class="{'updateText':customerProfile.BalanceAmount != updateCustomerInfo.BalanceAmount}" type="text" name="balanceAmount" ng-disabled="isBalanceAmountLimitDisable" class="form-control" id="" ng-model="customerProfile.BalanceAmount" maxlength="13" ng-pattern="/^0*(?!0\d|$)\d{1,8}(\.\d{1,4})?$/" ng-change="changeBalanceAmountLimit(customerProfile.BalanceAmount)">
                                            <div class="help-block" ng-messages="updateCustomer.balanceAmount.$error" ng-if="updateCustomer.balanceAmount.$touched">
                                                <p ng-message="pattern" class="text-error">{{balanceAmountInvalid}}</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
