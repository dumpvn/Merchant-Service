<script type="text/ng-template" id="viewCustomerProfile">
    <div class="modal-header">
        <h3 class="modal-title"><span string-replace="customerdetails"></span></h3>
    </div>
    <form name="updateCustomer">
        <div class="modal-body">
            <div class="form-horizontal">
                <div ng-class="{'disabled':customerProfile.IsDisabled}" class="row m-0">
                    <div ng-if="customerProfile.IsAllowReview">
                        <div ng-if="customerProfile.IsUpdated">
                            <h4><span string-replace="differencedetails"></span></h4>
                            <hr />
                            <div ng-class="{'enabled': customerProfile.IsAllowEnabled}" class="row">
                                <div class="col-lg-6">
                                    <div class="form-group">
                                        <label class="control-label col-sm-4 p-0"><span string-replace="name"></span>:</label>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control" maxlength="255" id="" name="name" ng-class="{'updateText':customerProfile.Name != updateCustomerInfo.Name}" ng-model="customerProfile.Name">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-sm-4 p-0"><span string-replace="address"></span>:</label>
                                        <div class="col-sm-8">
                                            <textarea class="form-control" name="address" maxlength="350" ng-class="{'updateText':customerProfile.Address != updateCustomerInfo.Address}" ng-model="customerProfile.Address"></textarea>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-sm-4 p-0"><span string-replace="email"></span>:</label>
                                        <div class="col-sm-8">
                                            <input type="email" maxlength="255" ng-class="{'updateText':customerProfile.Email != updateCustomerInfo.Email}" class="form-control" name="email" ng-model="customerProfile.Email" id="">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-sm-4 p-0"><span string-replace="fax"></span>:</label>
                                        <div class="col-sm-8">
                                            <input type="text" maxlength="15" ng-class="{'updateText':customerProfile.Fax != updateCustomerInfo.Fax}" class="form-control" id="" name="fax" ng-model="customerProfile.Fax">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-sm-4 p-0"><span string-replace="pricecategory"></span>:</label>
                                        <div class="col-sm-8">
                                            <ui-select ng-model="customerProfile.PriceCategory" name="category">
                                                <ui-select-match placeholder="Select Price Category">{{$select.selected.Category}}</ui-select-match>
                                                <ui-select-choices repeat="category.Id as category in priceCategoryCollection | filter: $select.search">
                                                    <div ng-bind-html="category.Category | highlight: $select.search"></div>
                                                </ui-select-choices>
                                            </ui-select>

                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="form-group">
                                        <label class="control-label col-sm-4 p-0"><span string-replace="mobile"></span>:</label>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control" maxlength="15" name="mobile" ng-class="{'updateText':customerProfile.Mobile != updateCustomerInfo.Mobile}" ng-model="customerProfile.Mobile" id="">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-sm-4 p-0"><span string-replace="phone"></span>:</label>
                                        <div class="col-sm-8">
                                            <input type="text" maxlength="15" ng-class="{'updateText':customerProfile.Phone != updateCustomerInfo.Phone}" class="form-control" name="phone" ng-model="customerProfile.Phone" id="">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-sm-4 p-0"><span string-replace="zipcode"></span>:</label>
                                        <div class="col-sm-8" ng-class="{'has-error': updateCustomer.zipcode.$touched && updateCustomer.zipcode.$invalid}">
                                            <input type="text" ng-class="{'updateText':customerProfile.Zipcode != updateCustomerInfo.Zipcode}" class="form-control" id="" name="zipcode" ng-model="customerProfile.Zipcode" maxlength="15" pattern="[a-zA-Z0-9-]*[a-zA-Z0-9-]{4,15}">
                                            <div class="help-block" ng-messages="updateCustomer.zipcode.$error" ng-if="updateCustomer.zipcode.$touched">
                                                <p ng-message="pattern" class="text-error">{{zipCodeInvalid}}</p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-sm-4 p-0"><span string-replace="amountlimit"></span>:</label>
                                        <div class="col-sm-8" ng-class="{'has-error': updateCustomer.amountLimit.$touched && updateCustomer.amountLimit.$invalid}">
                                            <input ng-class="{'updateText':customerProfile.AmountLimit != updateCustomerInfo.AmountLimit}" ng-disabled="isAmountLimitDisable" name="amountLimit" type="text" class="form-control" id="" ng-model="customerProfile.AmountLimit" maxlength="13" ng-pattern="/^0*(?!0\d|$)\d{1,8}(\.\d{1,4})?$/" ng-change="changeAmountLimit(customerProfile.AmountLimit)">
                                            <div class="help-block" ng-messages="updateCustomer.amountLimit.$error" ng-if="updateCustomer.amountLimit.$touched">
                                                <p ng-message="pattern" class="text-error">{{amountLimitInvalid}}</p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
